{{#if error}}
  <h3>{{error.message}}</h3>
{{/if}}

  <h1>Nomina de empleados</h1>
  <div>
    <table>
      <thead>
        <tr>
          <td>ID</td>
          <td>Nombre</td>
          <td>Apellido</td>
          <td>DNI</td>
          <td>Domicilio</td>
          <td>Sueldo base</td>
          <td>Puesto</td>
          <td>Telefono</td>
          <td>E-mail</td>
          <td>Editar</td>
          <td>Sueldo</td>
        </tr>
      </thead>

      <tbody>
        {{#each detallesFormat}}
          <tr>
            <td>{{id}}</td>
            <td>{{nombre}}</td>
            <td>{{apellido}}</td>
            <td>{{dni}}</td>
            <td>{{domicilio}}</td>
            <td>${{sueldo}}</td>
            <td>{{puesto}}</td>
            <td>{{telefono}}</td>
            <td>{{email}}</td>
            <td><a href="/admin/editar_empleado/{{id}}">Editar</a></td>
            <td><a href="/admin/asistencias">Cargar</a></td>
          </tr>
        {{/each}}
      </tbody>

    </table>
  </div>

</div>